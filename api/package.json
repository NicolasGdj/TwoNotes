{
  "name": "backendsqlite",
  "version": "1.0.0",
  "description": "backendsqlite is the ultimate backend solution",
  "main": "src/server.js",
  "scripts": {
    "setup": "npm install && npm run doc && npm run updatedb",
    "updatedb": "node src/util/updatedb.js",
    "doc": "node src/util/swagger.js",
    "start": "npm run doc && node src/server.js",
    "startdev": "INSTANCE=dev node src/util/swagger.js && INSTANCE=dev nodemon src/server.js",
    "test": "DB=dbtest.sqlite npm run updatedb && INSTANCE=test DB=dbtest.sqlite jest --runInBand",
    "lint": "npx eslint '**/*.js'",
    "lint:fix": "npx eslint --fix '**/*.js'"
  },
  "keywords": [
    "express",
    "backend",
    "backendsqlite"
  ],
  "license": "UNLICENSED",
  "dependencies": {
    "argon2": "^0.28.5",
    "axios": "^0.27.2",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-custom-error": "^1.8.4",
    "has-keys": "^0.1.0",
    "helmet": "^3.21.2",
    "http-status": "^1.3.2",
    "jws": "^4.0.0",
    "mandatoryenv": "^1.1.1",
    "mariadb": "^2.1.1",
    "module-alias": "^2.2.2",
    "morgan": "^1.9.1",
    "node-fetch": "^3.2.4",
    "nodemon": "^2.0.16",
    "react-native-dotenv": "^3.3.1",
    "rotating-file-stream": "^1.4.6",
    "sequelize": "^5.19.2",
    "socket.io": "^4.5.1",
    "sqlite3": "^5.0.2",
    "supertest": "^6.2.3",
    "swagger-autogen": "^2.18.8",
    "swagger-ui-express": "^4.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/eslint-parser": "^7.17.0",
    "@typescript-eslint/parser": "^5.23.0",
    "eslint": "^7.32.0",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.2.0",
    "jest": "^27.5.1",
    "prettier": "^2.6.2"
  }
}
